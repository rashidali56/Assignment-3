#include <iostream>

using namespace std;

int main()
{
    int a, b;

    cout << "Enter first number:" << endl;
    cin >> a;

    cout << "Enter second number:" << endl;
    cin >> b;

    int gcd = 1;

    for (int i = 1; i <= a && i <= b; i++)
    {
        if (a % i == 0 && b % i == 0)
        {
            gcd = i;
        }
    }

    int lcm = 0;

    for (int i = 1; i <= a * b; i++)
    {
        if (i % a == 0 && i % b == 0)
        {
            lcm = i;
            break;
        }
    }

    cout << "GCD is: " << gcd << endl;
    cout << "LCM is: " << lcm << endl;

    return 0;
}
